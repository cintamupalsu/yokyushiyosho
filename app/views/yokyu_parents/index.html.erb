<% if logged_in? %>
  <div class="row">
    <aside class="col-md-3">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="yokyushiyosho">
        <%= render 'yokyushiyosho/sidebar' %>
        <%= render 'sidebar' %>
      </section>
    </aside>
    
    <div class="col-md-9">
      <h3>要求仕様書列</h3>

      <%= will_paginate %>
      <ul class="list-group">
        <% @yokyu_parents.each do |yokyu_parent| %>
          <%= render yokyu_parent %>
        <% end %>
      </ul>
      <%= will_paginate %>      　
    </div>
  </div>
<% end %>